	<queryString>
		<![CDATA[SELECT clave_contrato, fecha, ref, clave_cliente,
(select nombre from pagarex_cliente where clave_cliente=pagarex_contrato.clave_cliente) as nombre_cliente,
(select rfc from pagarex_cliente where clave_cliente=pagarex_contrato.clave_cliente) as rfc_cliente,
(select direccion_fiscal from pagarex_cliente where clave_cliente=pagarex_contrato.clave_cliente) as direccion_fiscal_cliente,
(select cuenta from fx_cuenta where clave_cuenta=pagarex_contrato.clave_cuenta) as activo,
clave_tipo_contrato, importe, clave_moneda, tipo_cambio, tasa_interes, numero_pagares, (select ciclo from pagarex_ciclo_agricola where clave_ciclo=pagarex_contrato.clave_ciclo) as ciclo, toneladas_promesa,
hectareas,lugar_entrega,fecha_entrega, testigo
FROM pagarex_contrato
 where clave_contrato=$P{clave_contrato}]]>
	</queryString>
